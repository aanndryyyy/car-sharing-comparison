{
	"name": "car-sharing-comparison",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "npx jest --coverage",
		"fix": "npx eslint --fix \"{,!(node_modules|build)/**/}*.{ts,svelte}\"",
		"format": "npx prettier --write \"{,!(node_modules|build)/**/}*.{ts,svelte}\""
	},
	"devDependencies": {
		"@babel/core": "^7.21.5",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-typescript": "^7.22.5",
		"@rgossiaux/svelte-headlessui": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/forms": "^0.5.3",
		"@types/google.maps": "^3.53.1",
		"@types/jest": "^29.5.1",
		"@types/rollup-plugin-svelte-svg": "^1.0.3",
		"@types/svelte-range-slider-pips": "^2.0.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.40.0",
		"eslint-plugin-svelte": "^2.33.1",
		"jest": "^29.5.0",
		"postcss": "^8.4.21",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.0",
		"rollup-plugin-svelte-svg": "^1.0.0-beta.6",
		"svelte": "^4.2.0",
		"svelte-check": "^3.0.1",
		"svelte-hero-icons": "^5.0.0",
		"svelte-popperjs": "^1.3.2",
		"svelte-range-slider-pips": "^2.1.1",
		"tailwindcss": "^3.2.7",
		"ts-jest": "^29.1.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@babel/eslint-parser": "^7.21.8",
		"@googlemaps/js-api-loader": "^1.15.2",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"babel-install": "^2.1.0",
		"intl-messageformat": "^10.5.2",
		"postcss-import": "^15.1.0",
