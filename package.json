		"svelte-jester": "^3.0.0",
		"svelte-simple-modal": "^1.5.2",
		"svelte-transition": "^0.0.10",
		"svelte-use-click-outside": "^1.0.0",
		"tailwind-scrollbar-hide": "^1.1.7",
		"vite-tsconfig-paths": "^4.2.0"
	},
	"overrides": {
		"@rgossiaux/svelte-headlessui": {
			"svelte": "$svelte"
		}
	}
}
